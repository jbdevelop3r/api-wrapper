<h2> Miss Universe: </h2>
<table class="table">
  <tr>
    <th scope="col">Title</th>
    <th scope="col">First Name</th>
    <th scope="col">Last Name</th>
    <th scope="col">email</th>
    <th scope="col">avatar</th>
  </tr>
  <tr>
    <td><%= @random["results"][0]["name"]["title"]%></td>
    <td><%= @random["results"][0]["name"]["first"]%></td>
    <td><%= @random["results"][0]["name"]["last"]%></td>
    <td><%= @random["results"][0]["email"]%></td>
    <td><%=link_to "See picture", @random["results"][0]["picture"]["large"]%></td>
  </tr>
</table>


<h2> Top 5: </h2>
<table class="table">
  <tr>
    <th scope="col">Title</th>
    <th scope="col">First Name</th>
    <th scope="col">Last Name</th>
    <th scope="col">email</th>
    <th scope="col">avatar</th>
  </tr>
    <% @selected.each do |selected |%>
  <tr>
    <td><%= selected[1][0]%></td>
  </tr>
    <% end %>
</table>

